.container {
  height: 100% !important;
}

.left-content {
  width: 20%;
  height: 100%;
}

.right-content {
  width: 80%;
  height: 100%;
}

.nav-tab {
  height: 80rpx;
}

.search {
  width: 30rpx;
  height: 30rpx;
}

.lineleft {
  position: relative;
}

.lineleft::before {
  content: '';
  width: 10rpx;
  height: 80rpx;
  background: #ff6235;
  position: absolute;
  left: 0rpx;
}

.menu-list-item {
  width: 100%;
  height: 260rpx;
  margin-top: 20rpx;
}

.menu-list-item>image {
  width: 140rpx;
  height: 140rpx;
  margin-left: 15rpx;
  background: black;
}

.item-info {
  width: 65%;
  height: 85%;
  margin-right: 15rpx;
  background: #fff;
}

.laber {
  width: 80rpx;
  height: 40rpx;
  line-height: 40rpx;
}

.num>image,.icon {
  width: 40rpx;
  height: 40rpx;
}

.num>input,.nums {
  width: 65rpx;
  height: 24rpx;
  margin: 0 20rpx;
}

.shop-car {
  width: 195rpx;
  height: 80rpx;
  border-radius: 40rpx;
  border: 1rpx solid #000;
  position: fixed;
  left: 25rpx;
  bottom: 20rpx;
  z-index: 500;
}

.shop-car>image {
  width: 40rpx;
  height: 40rpx;
}

.g_nums {
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  border: 6rpx solid #fff;
  background: #dea482;
  position: fixed;
  left: 180rpx;
  bottom: 72rpx;
  z-index: 600;
}

.price-line {
  width: 750rpx;
  height: 90rpx;
  position: fixed;
  bottom: 0rpx;
  z-index: 200;
}

.btn {
  width: 30%;
  height: 90rpx;
  line-height: 90rpx;
  background: #dea482;
}

.car-content {
  width: 750rpx;
  height: 450rpx;
  position: fixed;
  bottom: 93rpx;
  z-index: 200;
}

.title-line {
  width: 100%;
  height: 59rpx;
  border-bottom: 1rpx solid #666;
}

.title-icon {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}

.cars {
  width: 100%;
  height: 391rpx;
}

.car-item {
  width: 100%;
  height: 115rpx;
  border-bottom: 1rpx solid #666;
}

.car-item >view {
  height: 50rpx;
}

.cars>view:last-child {
  border: none;
}

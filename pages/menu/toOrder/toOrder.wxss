page {
  background: #f5f5f5;
}

.container>view {
  width: 90%;
  margin-top: 40rpx;
}

.goods>view {
  width: 95%;
}

.goods>view:nth-child(2)>view {
  width: 100%;
  padding: 20rpx 0;
}

.go {
  width: 24rpx;
  height: 24rpx;
}

.f-line {
  width: 100% !important;
  height: 100rpx;
  position: fixed;
  bottom: 0rpx;
}

.f-line>view {
  width: 170rpx;
  height: 65rpx;
  line-height: 65rpx;
  margin: 0 20rpx;
  border-radius: 35rpx;
}

.good-img {
  width: 100rpx;
  height: 100rpx;
  background: black;
}

.good-info {
  width: 80%;
}

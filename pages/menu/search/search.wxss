page {
  background: #f5f5f5;
}

.search-line {
  width: 100%;
  height: 99rpx;
  border-bottom: 1rpx solid #f5f5f5;
  position: fixed;
  top: 0rpx;
}

.search-line>input {
  width: 84%;
  height: 60rpx;
  line-height: 60rpx;
}

.empyt {
  width: 380rpx;
  height: 380rpx;
  margin-top: 150rpx;
}

.search-list-item {
  width: 100%;
  height: 230rpx;
  margin-bottom: 20rpx;
}

.search-list-item>image {
  width: 170rpx;
  height: 170rpx;
  margin-left: 20rpx;
}

.search-list-item>view {
  width: 70%;
  height: 80%;
  margin-right: 20rpx;
}

.num>image {
  width: 40rpx;
  height: 40rpx;
}

.num>input {
  width: 65rpx;
  height: 24rpx;
  margin: 0 20rpx;
}

.sk-content {
  width: 100%;
  height: 1000rpx;
  position: fixed;
  bottom: 0rpx;
  z-index: 500;
}

.sk-content>view:first-child {
  width: 100%;
  height: 220rpx;
  position: relative;
}

.close {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
}

.goods-img {
  width: 180rpx;
  height: 180rpx;
  margin: 0 20rpx;
}

.good-info {
  width: 60%;
  height: 180rpx;
}

.sk-content>view:last-child {
  width: 90%;
  height: 75rpx;
  line-height: 75rpx;
  background: #2295fe;
  margin-top: 20rpx;
}

.sk-content>scroll-view {
  width: 100%;
  height: 670rpx;
}

.sk-line {
  width: 95%;
  margin: 0 auto;
  margin-top: 40rpx;
}

.sk-line:last-child {
  margin-bottom: 40rpx;
}

.sk-line>text {
  margin-bottom: 40rpx;
}

.laber {
  width: 160rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin: 0 15rpx;
}

.active {
  background: #d3e7ff;
  color: #1faff1;
}

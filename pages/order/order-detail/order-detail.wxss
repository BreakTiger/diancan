page {
  background: #f5f5f5;
}

.container>view, .good {
  width: 90%;
}

.go-icon {
  width: 30rpx;
  height: 30rpx;
  margin-left: 20rpx;
}

.goods {
  width: 150rpx;
  height: 150rpx;
}

.good>view {
  width: 70%;
  height: 150rpx;
}

.line {
  width: 90%;
  padding: 20rpx 0;
  border-top: 1rpx solid #f5f5f5;
}

.f-line {
  position: fixed;
  bottom: 0rpx;
  width: 100% !important;
  height: 100rpx;
}

.btn {
  padding: 10rpx;
  border: 1rpx solid #000;
  margin-right: 20rpx;
}
